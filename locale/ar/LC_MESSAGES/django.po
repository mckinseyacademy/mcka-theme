msgid "55 East 52nd Street"
msgstr "55 East 52nd Street"

#: templates/privacy.haml:133 templates/terms_of_service.haml:116
msgid "New York, NY 10022"
msgstr "New York, NY 10022"

#: templates/registration/password_reset_complete.haml:3
msgid "Password Reset Complete"
msgstr "اكتمال إعادة تعيين كلمة المرور"

#: templates/registration/password_reset_complete.haml:6
